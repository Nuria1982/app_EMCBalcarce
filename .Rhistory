pp_acum <- datos %>%
filter(Mes %in% c("abril", "mayo", "junio", "julio", "agosto", "septiembre")) %>%
group_by(Año) %>%
summarise(Precipitacion_Acumulada = sum(Precipitacion_Pluviometrica, na.rm = TRUE))
etiquetas_10_anios <- pp_acum %>%
filter(Año %% 10 == 0) %>%
